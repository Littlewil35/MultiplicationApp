<?xml version="1.1" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.w.k.multiplicationapp">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <supports-screens android:normalScreens="false"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/mult_max_icon2"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name="com.w.k.multiplicationapp.multiplier"
            android:parentActivityName="com.w.k.multiplicationapp.MainActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            >

        </activity>
        <activity android:name="com.w.k.multiplicationapp.MainActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>